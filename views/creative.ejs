<section id="quizSelection">
    <div class="container-fluid">
        <div class="row no-gutter">
            <% Object.keys(quizzes).forEach(function(quiz) { %>
            <div class="col-lg-<%= Object.keys(quizzes).length %> col-sm-<%= Object.keys(quizzes).length %>">
                <a href="#" class="quiz-select" data-quiz="<%= quiz %>">
                    <img src="img/<%= quizzes[quiz].imageName %>.jpg" class="img-responsive" alt="">
                    <div class="project-name">
                        <%= quiz %>
                    </div>
                </a>
            </div>
            <% }); %>
        </div>
    </div>
</section>
<section id="quizBox" class="hidden">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2 class="section-heading question">Question</h2>
                <hr class="primary">
            </div>
        </div>
    </div>
    <div class="container">
        <div class="row">
            <div class="col-lg-3 col-md-6 text-center well" data-answer-letter="a">
                <div class="service-box">
                    <h3 class="answer answer-a">A</h3>

                    <p class="text-muted"></p>
                </div>
            </div>
            <div class="col-lg-3 col-md-6 text-center well" data-answer-letter="b">
                <div class="service-box">
                    <h3 class="answer answer-b">B</h3>

                    <p class="text-muted"></p>
                </div>
            </div>
            <div class="col-lg-3 col-md-6 text-center well" data-answer-letter="c">
                <!-- <div class="service-box"> -->
                    <h3 class="answer answer-c">C</h3>

                    <!-- <p class="text-muted"></p> -->
                <!-- </div> -->
            </div>
        </div>
    </div>

    <div class="container">
        <div class="row">
            <!-- <div class="col-lg-8 col-lg-offset-2 text-center"> -->
                <button type="button" class="btn btn-success btn-lg submit" >Submit</button>
          <!--   </div> -->
        </div>
    </div>
</section>

